<%- include("../partials/header") %> 

<header class="gradient">
    <h1>FAQ</h1>
</header>

<div class="container">
    <ul class="faq">
        <% faq.forEach(f => { %> 
            <li>
                <div class="toggle">
                    <h3><strong>Q.</strong> <%= f.question %></h3>
                    <i class="fas fa-plus"></i>
                </div>
                <p><strong>A.</strong> <%- f.answer %></p>
            </li>
        <% }) %> 
    </ul>
</div>

<%- include("../partials/footer") %> 